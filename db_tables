
CREATE TABLE STUDENT (USN VARCHAR(20) PRIMARY KEY, NAME VARCHAR(50), JOINING_YEAR DATE, EXP_GRAD_YEAR DATE, PHONE VARCHAR(15), EMAIL VARCHAR(60)); 

CREATE TABLE PARENT (PARENT_ID INTEGER PRIMARY KEY, S_USN VARCHAR(20), PHONE VARCHAR(15), EMAIL VARCHAR(60), FOREIGN KEY (S_USN) REFERENCES STUDENT(USN));

CREATE TABLE TEACHER (TEACHER_ID INTEGER PRIMARY KEY, TEACHER_NAME VARCHAR(50), DEPT_ID INTEGER, FOREIGN KEY (DEPT_ID) REFERENCES DEPARTMENT(DEPT_ID));

CREATE TABLE SUBJECT (SUBJECT_ID INTEGER PRIMARY KEY AUTO INCREMENT, SUBJECT_NAME VARCHAR(10));

CREATE TABLE MARKS (S_USN VARCHAR(20), S_SUBJECT_ID INTEGER, MARKS INTEGER, PRIMARY KEY (S_USN, S_SUBJECT_ID), FOREIGN KEY(S_USN) REFERENCES STUDENT(USN), FOREIGN KEY(S_SUBJECT_ID) REFERENCES SUBJECT(SUBJECT_ID));

CREATE TABLE ATTENDANCE (S_USN VARCHAR(20), S_SUBJECT_ID INTEGER, ATTENDANCE INTEGER, PRIMARY KEY (S_USN, S_SUBJECT_ID), FOREIGN KEY(S_USN) REFERENCES STUDENT(USN), FOREIGN KEY(S_SUBJECT_ID) REFERENCES SUBJECT(SUBJECT_ID));





